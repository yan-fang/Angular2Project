{
  "name": "ease-web",
  "version": "1.0.0-alpha",
  "description": "Angular 2 Shell",
  "main": "index.js",
  "scripts": {
    "build": "yarn cleanup && webpack",
    "cleanup": "rm -rf _dist",
    "commit": "git-cz",
    "compile": "tsc",
    "cover": "karma start ./karma.conf.js",
    "cover:watch": "karma start ./karma.conf.js --single-run false",
    "dashboard": "webpack-dashboard -- yarn dev",
    "dev": "yarn cleanup && webpack-dev-server --inline --colors --progress --port 3000  --content-base src",
    "docs": "./docs/init.sh",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "lint": "tslint \"src/**/*.ts\"",
    "ngc": "yarn cleanup && ngc --p tsconfig-build.json",
    "precommit": "yarn lint && yarn compile && yarn test",
    "pree2e": "yarn webdriver-update",
    "test": "karma start ./karma.conf.js --reporters spec",
    "test:watch": "karma start ./karma.conf.js --single-run false --reporters spec",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start"
  },
  "repository": {
    "type": "git",
    "url": "git@github.kdc.capitalone.com:ease-ui/ease-web-v2.git"
  },
  "keywords": [
    "typescript",
    "angular",
    "shell",
    "ease"
  ],
  "engines": {
    "node": "6.9.5"
  },
  "author": "Capital One",
  "license": "ISC",
  "devDependencies": {
    "@angular/compiler": "2.4.4",
    "@angular/compiler-cli": "2.4.4",
    "@angular/platform-server": "2.4.4",
    "@c1/git-commit-conventions": "git+https://github.kdc.capitalone.com/ease-ui/git-commit-conventions#v3.2.1",
    "@ngtools/webpack": "^1.2.11",
    "@types/core-js": "^0.9.35",
    "@types/jasmine": "^2.5.41",
    "@types/node": "^7.0.2",
    "@types/reflect-metadata": "^0.0.5",
    "@types/zone.js": "^0.0.27",
    "angular-router-loader": "^0.5.0",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "browser-sync": "^2.18.7",
    "browser-sync-webpack-plugin": "^1.1.4",
    "codelyzer": "^2.0.0-beta.4",
    "commitizen": "^2.9.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "html-webpack-plugin": "^2.28.0",
    "jasmine": "^2.5.3",
    "jasmine-spec-reporter": "^3.2.0",
    "json-loader": "^0.5.4",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-scss-preprocessor": "^2.0.0",
    "karma-spec-reporter": "^0.0.26",
    "karma-typescript": "https://github.com/melxx001/karma-typescript#2.1.8-feb15",
    "node-sass": "3.7.0",
    "protractor": "^5.1.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^5.0.1",
    "to-string-loader": "^1.1.5",
    "ts-node": "^2.1.0",
    "tslint": "^4.3.1",
    "typescript": "^2.1.5",
    "webpack": "^2.2.0",
    "webpack-dashboard": "^0.2.1",
    "webpack-dev-server": "^2.2.0"
  },
  "dependencies": {
    "@angular/common": "2.4.4",
    "@angular/core": "2.4.4",
    "@angular/forms": "2.4.4",
    "@angular/http": "2.4.4",
    "@angular/platform-browser": "2.4.4",
    "@angular/platform-browser-dynamic": "2.4.4",
    "@angular/router": "3.4.4",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.0",
    "@ngrx/router-store": "^1.2.5",
    "@ngrx/store": "^2.2.1",
    "@ngrx/store-devtools": "^3.2.3",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.0.3",
    "source-map-explorer": "^1.3.3",
    "zone.js": "^0.7.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@c1/git-commit-conventions"
    }
  }
}
